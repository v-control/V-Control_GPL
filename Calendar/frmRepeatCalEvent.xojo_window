	#tag Event
		Sub Open()
		  dim i as integer
		  
		  frmRepeatCalEvent.Height = 230
		  
		  //Day(s)
		  cmbInterval.AddRow("Day(s)")
		  cmbInterval.AddRow("Week(s)")
		  //cmbInterval.AddRow("Month(s)")
		  //cmbInterval.AddRow("Year(s)")
		  cmbInterval.ListIndex = 0
		  
		  cmbDayDuration.AddRow("For")
		  cmbDayDuration.AddRow("To")
		  cmbDayDuration.AddRow("Forever")
		  cmbDayDuration.ListIndex = 0
		  
		  spnDayInterval.Min = 1
		  spnDayInterval.Max = 365
		  spnDayInterval.SetVal(1)
		  spnDayTimes.Min = 1
		  spnDayTimes.Max = 3650
		  spnDayTimes.SetVal(1)
		  
		  //Week(s)
		  cmbWeekDuration.AddRow("For")
		  cmbWeekDuration.AddRow("To")
		  cmbWeekDuration.AddRow("Forever")
		  cmbWeekDuration.ListIndex = 0
		  
		  spnWeekInterval.Min = 1
		  spnWeekInterval.Max = 56
		  spnWeekInterval.SetVal(1)
		  spnWeekTimes.Min = 1
		  spnWeekTimes.Max = 560
		  spnWeekTimes.SetVal(1)
		  
		  
		  //Month(s)
		  spnMonthInterval.Min = 1
		  spnMonthInterval.Max = 120
		  spnMonthInterval.SetVal(1)
		  
		  spnMonthTimes.Min = 1
		  spnMonthTimes.Max = 120
		  spnMonthTimes.SetVal(1)
		  
		  cmbMonthDay.AddRow("First")
		  cmbMonthDay.AddRow("Second")
		  cmbMonthDay.AddRow("Third")
		  cmbMonthDay.AddRow("Fourth")
		  cmbMonthDay.AddRow("Last")
		  cmbMonthDay.AddRow("Day")
		  cmbMonthDay.ListIndex = 0
		  
		  cmbWeekdays.AddRow("Sun")
		  cmbWeekdays.AddRow("Mon")
		  cmbWeekdays.AddRow("Tue")
		  cmbWeekdays.AddRow("Wen")
		  cmbWeekdays.AddRow("Thu")
		  cmbWeekdays.AddRow("Fri")
		  cmbWeekdays.AddRow("Sat")
		  cmbWeekdays.ListIndex = 0
		  
		  for i = 1 to 31
		    cmbDays.AddRow(str(i))
		  next
		  cmbDays.ListIndex = 0
		  
		  cmbMonthDuration.AddRow("For")
		  cmbMonthDuration.AddRow("To")
		  cmbMonthDuration.AddRow("Forever")
		  cmbMonthDuration.ListIndex = 0
		  
		  //Year(s)
		  spnYearInterval.Min = 1
		  spnYearInterval.Max = 10
		  spnYearInterval.SetVal(1)
		  
		  spnYearTimes.Min = 1
		  spnYearTimes.Max = 10
		  spnYearTimes.SetVal(1)
		  
		  cmbYearDuration.AddRow("For")
		  cmbYearDuration.AddRow("To")
		  cmbYearDuration.AddRow("Forever")
		  cmbYearDuration.ListIndex = 0
		  
		End Sub
	#tag EndEvent


