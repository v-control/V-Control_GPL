	#tag Event
		Sub Action()
		  dim res as string
		  dim d as new date
		  dim b as boolean
		  
		  if cmbInterval.ListIndex = 0 then
		    res = "DAY;"+str(spnDayInterval.GetVal)+";"+str(cmbDayDuration.ListIndex)
		    if cmbDayDuration.ListIndex = 0 then res = res + ";"+str(spnDayTimes.GetVal)
		    if cmbDayDuration.ListIndex = 1 then 
		      b = ParseDate(txtDayToDate.Text,d)
		      if b = false then
		        MsgBox("Invalid date format. Use a date format that respect your current locale settings")
		        exit sub
		      end
		    end
		    res = res + ";"+txtDayToDate.Text
		  end
		  
		  if cmbInterval.ListIndex = 1 then
		    res = "WEEK;"+str(spnWeekInterval.GetVal)+";"
		    if CheckBox1.Value = true then
		      res = res+"1"
		    else
		      res = res+"0"
		    end
		    if CheckBox2.Value = true then
		      res = res+"1"
		    else
		      res = res+"0"
		    end
		    if CheckBox3.Value = true then
		      res = res+"1"
		    else
		      res = res+"0"
		    end
		    if CheckBox4.Value = true then
		      res = res+"1"
		    else
		      res = res+"0"
		    end
		    if CheckBox5.Value = true then
		      res = res+"1"
		    else
		      res = res+"0"
		    end
		    if CheckBox6.Value = true then
		      res = res+"1"
		    else
		      res = res+"0"
		    end
		    if CheckBox7.Value = true then
		      res = res+"1;"
		    else
		      res = res+"0;"
		    end
		    res = res +str(cmbWeekDuration.ListIndex)
		    if cmbWeekDuration.ListIndex = 0 then res = res + ";"+str(spnWeekTimes.GetVal)
		    if cmbWeekDuration.ListIndex = 1 then 
		      b = ParseDate(txtWeekToDate.Text,d)
		      if b = false then
		        MsgBox("Invalid date format. Use a date format that respect your current locale settings")
		        exit sub
		      end
		    end
		    res = res + ";"+txtWeekToDate.Text
		  end
		  frmCalendarEvent.GUICalendar.SetRepeat(res)
		  
		  frmRepeatCalEvent.Close
		End Sub
	#tag EndEvent
#tag EndEvents
