#tag Events txtBin
	#tag Event
		Function KeyDown(Key As String) As Boolean
		  if key = chr(13) or key = chr(3) then
		    cmdFromBin.Push
		  end
		End Function
	#tag EndEvent
#tag EndEvents
#tag Events cmdFromBin
	#tag Event
		Sub Action()
		  dim i,x,m as integer
		  dim s as string
		  
		  m = 1 
		  i = 0
		  for x = len(txtBin.Text) DownTo 1
		    s = mid(txtBin.Text,x,1)
		    if s = "1" then i = i + m
		    m = m*2
		  next
		  
		  txtDec.Text = str(i)
		  txtHex.Text = Hex(i)
		  
		  //Convert To ASCII
		  if len(txtHex.Text) / 2 <> len(txtHex.Text) \ 2 then txtHex.Text = "0" + txtHex.Text
		  txtASCI.Text = HexStringToString(txtHex.Text)
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events txtHex
	#tag Event
		Function KeyDown(Key As String) As Boolean
		  if key = chr(13) or key = chr(3) then
		    cmdFromHex.Push
