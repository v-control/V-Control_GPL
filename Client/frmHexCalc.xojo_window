#tag Events txtASCI
	#tag Event
		Function KeyDown(Key As String) As Boolean
		  if key = chr(13) or key = chr(3) then
		    cmdFromASCII.Push
		  end
		End Function
	#tag EndEvent
#tag EndEvents
#tag Events cmdFromHex
	#tag Event
		Sub Action()
		  dim i as integer
		  
		  
		  
		  //Convert To ASCII
		  if len(txtHex.Text) / 2 <> len(txtHex.Text) \ 2 then txtHex.Text = "0" + txtHex.Text
		  txtASCI.Text = HexStringToString(txtHex.Text)
		  
		  //to dec
		  i = val("&h"+txtHex.Text)
		  txtDec.Text = str(i)
		  
		  //to bin
		  txtBin.Text = bin(i)
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events cmdFromDec
	#tag Event
		Sub Action()
		  dim i as integer
		  
		  i = val(txtdec.Text)
		  txtHex.Text = Hex(i)
		  
		  //Convert To ASCII
		  if len(txtHex.Text) / 2 <> len(txtHex.Text) \ 2 then txtHex.Text = "0" + txtHex.Text
		  txtASCI.Text = HexStringToString(txtHex.Text)
		  
		  
		  
		  //to bin
		  txtBin.Text = bin(i)
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events cmdFromASCII
	#tag Event
		Sub Action()
		  dim i as integer
		  
		  txtHex.Text = StringToHexString(txtASCI.Text)
		  i = val("&h"+txthex.Text)
		  
		  //to bin
		  txtBin.Text = bin(i)
		  txtDec.Text = str(i)
		End Sub
	#tag EndEvent
#tag EndEvents
