#tag WindowCode
	#tag Event
		Sub Open()
		  txtKeys.SetFocus
		End Sub
	#tag EndEvent


#tag EndWindowCode

#tag Events cmdClear
	#tag Event
		Sub Action()
		  txtKeys.Text = ""
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events txtKeys
	#tag Event
		Function KeyDown(Key As String) As Boolean
		  
		  me.Text = ConvertKeycodeToKeyname(Key)
		  Return true
		End Function
	#tag EndEvent
#tag EndEvents
#tag Events cmdCancel
	#tag Event
		Sub Action()
		  frmInsertKeycode.Close
