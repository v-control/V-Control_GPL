	#tag EndEvent
#tag EndEvents
#tag Events cmdOK
	#tag Event
		Sub Action()
		  if Globals.ptyModalPlaylist = true then
		    frmPlaylistModal.lstItems.Cell(frmPlaylistModal.lstItems.ListIndex,0) = txtKeys.Text
		  else
		    frmPlaylist.lstItems.Cell(frmPlaylist.lstItems.ListIndex,0) = txtKeys.Text
		  end
		  frmInsertKeycode.Close
		End Sub
	#tag EndEvent
#tag EndEvents
